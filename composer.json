{
    "name": "pkeogan/laravel-alpacajs",
    "description": "A package to build AlpacaJS forms via Laravel.",
    "keywords": ["laravel", "alpaca", "alpacajs"],
    "license": "MIT",
    "version": "dev-master",
    "authors": [
        {
            "name": "Peter Keogan",
            "email": "peter@pkeogan.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.2",
        "lightsaml/lightsaml": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Pkeogan\\LaravelAlpacaJS\\": "src"
        }
    },
    "extra": {
        "laravel": {
          "providers": [
            "Pkeogan\\LaravelAlpacaJS\\LaravelAlpacaJSServiceProvider"
          ],
			"aliases": {
                "AlpacaJS": "Pkeogan\\LaravelAlpacaJS\\AlpacaJSFacade"
            }
        }
    }
}
